{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CacheWarming": {
    "IntervalInMinutes": 30,
    "MaxPages": 1000,
    "BatchSize": 5,
    "MaxConcurrentOperations": 3,
    "RetryAttempts": 3,
    "DelayBetweenBatchesSeconds": 10,
    "ProcessIndividualVideos": false
  },
  "robos": {
    "ativar": "true"
  }
}
